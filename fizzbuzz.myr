use std
const main = {
    var i
    for i = 1; i <= 100; i++
        if i % 15 == 0
            std.put("FizzBuzz\n")
        elif i % 3 == 0
            std.put("Fizz\n")
        elif i % 5 == 0
            std.put("Buzz\n")
        else
            std.put("{}\n", i)
        ;;
    ;;
} 
