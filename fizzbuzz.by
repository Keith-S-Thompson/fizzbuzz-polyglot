#!/usr/bin/env bython

# Language:       Bython
# Web site:       https://github.com/mathialo/bython
# Last tested on: Ubuntu 22.04.4
# Requires:       pip3 install bython

for i in range(1, 101) {
    if i % 15 == 0 {
        print("FizzBuzz")
    }
    elif i % 3 == 0 {
        print("Fizz")
    }
    elif i % 5 == 0 {
        print("Buzz")
    }
    else {
        print(i)
    }
}
