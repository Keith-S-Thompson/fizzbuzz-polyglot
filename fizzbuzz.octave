#!/usr/bin/octave -q

% Language:       Octave (should be compatible with Matlab)
% Web site:       http://www.gnu.org/software/octave/
% Last tested on: Ubuntu 20.04
% Requires:       apt-get install octave

for i=1:100
    if (mod(i, 15) == 0)
        disp('FizzBuzz')
    elseif (mod(i, 3) == 0)
        disp('Fizz')
    elseif (mod(i, 5) == 0)
        disp('Buzz')
    else
        disp(int2str(i))
    endif
end
