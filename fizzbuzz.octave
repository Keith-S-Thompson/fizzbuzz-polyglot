#!/usr/bin/octave -q

% Language:       Octave (should be compatible with Matlab)
% Web site:       https://octave.org/
% Last tested on: Ubuntu 22.04.4
% Requires:       apt-get install octave

for i=1:100
    if (mod(i, 15) == 0)
        disp('FizzBuzz')
    elseif (mod(i, 3) == 0)
        disp('Fizz')
    elseif (mod(i, 5) == 0)
        disp('Buzz')
    else
        disp(int2str(i))
    endif
end
