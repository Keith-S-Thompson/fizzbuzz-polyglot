#!/usr/bin/nickle

# Language:       Nickle
# Web site:       https://nickle.org/
# Last tested on: Ubuntu 22.04.3
# Requires:       apt-get install nickle

for (i = 1; i <= 100; i ++) {
    if (i % 15 == 0) {
        printf("%s\n", "FizzBuzz");
    }
    else if (i % 3 == 0) {
        printf("%s\n", "Fizz");
    }
    else if (i % 5 == 0) {
        printf("%s\n", "Buzz");
    }
    else {
        printf("%d\n", i);
    }
}
exit(0);
