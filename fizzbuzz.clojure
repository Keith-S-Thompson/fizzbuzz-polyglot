#!/usr/bin/clojure

; Language:       Clojure
; Web site:       https://clojure.org/
; Last tested on: Ubuntu 24.04.2 LTS
; Requires:       apt-get install clojure

(defn line [n]
    (cond
        (= 0 (mod n 15)) "FizzBuzz"
        (= 0 (mod n  3)) "Fizz"
        (= 0 (mod n  5)) "Buzz"
        :else (format "%d" n)
    )
)

(defn print-line [n] (println (line n)))

(dorun (map print-line (range 1 101)))
