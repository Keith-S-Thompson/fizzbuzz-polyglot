#!/usr/bin/elvish

# Language:       Elvish
# Web site:       https://elvish.io/
# Last tested on: Ubuntu 20.04
# Requires:       apt-get install elvish

for i [ ( seq 1 100 ) ] {
    if (== (% $i 15) 0) {
        echo FizzBuzz
    } elif (== (% $i 3) 0) {
        echo Fizz
    } elif (== (% $i 5) 0) {
        echo Buzz
    } else {
        echo $i
    }
}
